from selenium.webdriver.common.desired_capabilities import DesiredCapabilities

class Test1():
  def setup_method(self, method):
    self.driver = webdriver.Remote(command_executor='http://localhost:4444/wd/hub', desired_capabilities=DesiredCapabilities.CHROME)
    self.vars = {}
  
  def teardown_method(self, method):
    self.driver.quit()
  
  def test_1(self):
    self.driver.get("https://www.almacgroup.com/")
    self.driver.set_window_size(1936, 1048)
    self.driver.find_element(By.CSS_SELECTOR, "a > .lazyloaded").click()
    self.driver.find_element(By.CSS_SELECTOR, ".next-top").click()
    self.driver.find_element(By.CSS_SELECTOR, ".prev-top").click()
    self.driver.find_element(By.CSS_SELECTOR, ".is-open:nth-child(1)").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-2923 span").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-3022 > .c-navbar__dropdown > span").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-2939 > .c-navbar__dropdown").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-193 span").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-196 span").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-3010 span").click()
    self.driver.find_element(By.CSS_SELECTOR, "#menu-item-200 > .c-navbar__dropdown").click()
    self.driver.close()
  
